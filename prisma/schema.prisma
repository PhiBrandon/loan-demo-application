generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model loanapplications {
  loanid        String?   @db.Uuid
  userid        String?   @db.Uuid
  appliedat     DateTime? @db.Date
  status        String?
  id            String    @id @db.Uuid
  type          String?
  propertytype  String?
  creditscore   Decimal?  @db.Decimal
  streetaddress String?
  propertyuse   String?
  city          String?
  postalcode    Decimal?  @db.Decimal
}

model loans {
  loanid       String  @id @db.Uuid
  loanissuer   String
  interestrate Decimal @db.Decimal
  product      String
}

model users {
  id       String  @id @db.Uuid
  email    String
  name     String?
  password String?
}

model usersInfo {
  email   String  @id
  name    String?
  address String?
  city    String?
  state   String?
}
